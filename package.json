{
  "name": "offline-aframe",
  "version": "0.1.0",
  "description": "offline A-Frame",
  "scripts": {
    "pack": "cross-env NODE_ENV=production aframe-packer run .",
    "build": "cross-env NODE_ENV=production webpack",
    "start": "webpack-dev-server --host 0.0.0.0",
    "lint": "semistandard -v | snazzy"
  },
  "repository": "chenzlabs/offline-aframe",
  "license": "MIT",
  "semistandard": {
    "ignore": [
      "**/vendor/**"
    ]
  },
  "dependencies": {
    "aframe": "aframevr/aframe#master",
    "aframe-assets": "aframevr/assets#gh-pages"
  },
  "devDependencies": {
    "cross-env": "^2.0.1",
    "semistandard": "^9.0.0",
    "snazzy": "^4.0.1",
    "webpack": "^1.13.2",
    "file-loader": "^0.11.2",
    "url-loader": "^0.5.9",
    "webpack-dev-server": "^1.16.1"
  }
}
